<%= render 'users/info', users: @users %>
<%= render 'books/error', books: @books %>
<%= render 'books/form', books: @books %>

<h1>Users</h1>
<% @users.each do |user| %>
<div>
  <%= attachment_image_tag user, :profile_image, format: "jpeg", fallback: "no_image.jpg", size: "40x40" %>
  <p>name  : <%= user.name %></p>
  <%= link_to "Show", user_path(user) %>
</div>
<% end %>